{
  "name": "vease",
  "version": "0.0.0",
  "description": "Desktop and cloud software for data visualization",
  "main": "dist-electron/main.js",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean_cache": "npx nuxi cleanup",
    "dev": "npm run dev:browser",
    "dev:browser": "node node_scripts/browser.js dev:nuxt",
    "dev:desktop": "npm run dev:nuxt",
    "dev:nuxt": "npm run clean_cache && npx nuxi@3.11.0 dev --no-fork",
    "build": "nuxi build --prerender",
    "build:browser": "cross-env BROWSER=true npm run build",
    "build:desktop": "npm run build && electron-builder --dir",
    "preview:browser": "nuxi preview",
    "generate": "nuxt generate",
    "compile:back": "node node_scripts/run_script_file.js compile back",
    "compile:viewer": "node node_scripts/run_script_file.js compile viewer",
    "compile:microservices": "concurrently 'npm run compile:back' 'npm run compile:viewer'",
    "install:back": "node node_scripts/run_script_file.js install back",
    "install:viewer": "node node_scripts/run_script_file.js install viewer",
    "install:microservices": "concurrently 'npm run install:back' 'npm run install:viewer'",
    "build:back": "node node_scripts/run_script_file.js back/build",
    "build:viewer": "node node_scripts/run_script_file.js viewer/build",
    "build:microservices": "concurrently 'npm run build:back' 'npm run build:viewer'",
    "package": "npm run build && electron-builder build --publish=never",
    "internal:browser": "node node_scripts/browser.js preview:browser",
    "test:e2e:browser": "npx playwright test --config ./tests/e2e/browser/playwright.config.js",
    "test:e2e:desktop": "npx playwright test --config ./tests/e2e/desktop/playwright.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Geode-solutions/Vease.git"
  },
  "keywords": [
    "Geology",
    "Visualization",
    "Web",
    "Desktop",
    "Meshing"
  ],
  "author": "Geode-solutions",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Geode-solutions/Vease/issues"
  },
  "homepage": "https://github.com/Geode-solutions/Vease#readme",
  "devDependencies": {
    "@nuxt/test-utils": "3.19.2",
    "@playwright/test": "1.54.1",
    "concurrently": "^9.2.1",
    "cross-env": "10.0.0",
    "electron": "36.4.0",
    "electron-builder": "26.0.12",
    "electron-playwright-helpers": "1.7.1",
    "nuxt-electron": "0.7.0",
    "playwright": "1.54.1",
    "std-env": "3.9.0",
    "vuetify-nuxt-module": "0.18.7"
  },
  "dependencies": {
    "@fontsource/michroma": "5.2.6",
    "@geode/opengeodeweb-back": "next",
    "@geode/opengeodeweb-front": "next",
    "@geode/opengeodeweb-viewer": "^1.11.4-rc.1",
    "@geode/vease-back": "next",
    "@geode/vease-viewer": "next",
    "broadcast-channel": "7.1.0",
    "bufferutil": "^4.0.9",
    "draggable": "4.2.0",
    "electron-updater": "6.6.2",
    "get-port-please": "3.2.0",
    "h3-formidable": "1.0.0",
    "nuxt": "3.13.2",
    "utf-8-validate": "^6.0.5",
    "vuetify": "3.8.12"
  }
}