{
  "name": "vease",
  "version": "0.0.0",
  "private": true,
  "description": "Desktop and cloud software for data visualization",
  "main": "dist-electron/main.js",
  "scripts": {
    "clean_cache": "npx nuxi cleanup",
    "browser": "concurrently --kill-others \"npm run run_microservices\" \"npm run dev\"",
    "desktop": "concurrently --kill-others \"npm run dev\"",
    "dev": "npm run clean_cache & npx nuxi@3.11.0 dev --no-fork",
    "generate": "nuxt generate",
    "compile_back": "node node_scripts/run_script_file.js compile back",
    "compile_viewer": "node node_scripts/run_script_file.js compile viewer",
    "compile_microservices": "npm run compile_back && npm run compile_viewer",
    "install_back": "node node_scripts/run_script_file.js install back",
    "install_viewer": "node node_scripts/run_script_file.js install viewer",
    "install_microservices": "npm run install_back && npm run install_viewer",
    "build_back": "node node_scripts/run_script_file.js back/build",
    "build_viewer": "node node_scripts/run_script_file.js viewer/build",
    "build_microservices": "npm run build_back && npm run build_viewer",
    "run_back": "/home/julien/Code/Vease/electron-server/back/venv/bin/vease-back --port 5000 --data_folder_path /tmp/vease --allowed_origin [*] --timeout 0",
    "run_viewer": "/home/julien/Code/Vease/electron-server/viewer/venv/bin/vease-viewer --port 1234 --data_folder_path /tmp/vease --timeout 0",
    "run_microservices": "concurrently --kill-others \"npm run run_back\" \"npm run run_viewer\"",
    "build": "nuxi build --prerender",
    "test": "vitest",
    "package": "npm run build && electron-builder build --publish=never",
    "test:e2e": "npx playwright test",
    "test:browser": "vitest --config=vitest.browser.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Geode-solutions/Vease.git"
  },
  "keywords": [
    "Geology",
    "Visualization",
    "Web",
    "Desktop",
    "Meshing"
  ],
  "author": "Geode-solutions",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Geode-solutions/Vease/issues"
  },
  "homepage": "https://github.com/Geode-solutions/Vease#readme",
  "devDependencies": {
    "@playwright/test": "^1.53.0",
    "@types/node": "^24.0.3",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/browser": "^3.2.4",
    "electron": "36.4.0",
    "electron-builder": "26.0.12",
    "pinia-logger": "^1.3.13",
    "playwright": "^1.53.0",
    "vitest": "^3.2.4",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vitest-browser-vue": "^0.2.0",
    "vitest-environment-nuxt": "^1.0.1",
    "vuetify-nuxt-module": "0.18.7"
  },
  "dependencies": {
    "@fontsource/michroma": "5.2.6",
    "@geode/opengeodeweb-back": "5.8.1",
    "@geode/opengeodeweb-front": "9.7.5",
    "@geode/opengeodeweb-viewer": "1.9.1",
    "@geode/vease-back": "1.4.12",
    "@geode/vease-viewer": "2.3.11",
    "broadcast-channel": "7.1.0",
    "concurrently": "^9.2.0",
    "draggable": "4.2.0",
    "electron-updater": "6.6.2",
    "get-port-please": "3.1.2",
    "h3-formidable": "1.0.0",
    "nuxt": "3.13.2"
  }
}